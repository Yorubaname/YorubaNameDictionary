@model int
@using Website.Resources
@using Website.Services.MultiLanguage
@inject Microsoft.Extensions.Localization.IStringLocalizer<Messages> Localizer
@inject ILanguageService LanguageService

<section id="searchform">
    <div class="jumbotron" id="home-banner">
        <div class="container">
            <h1>@Localizer["jumbotron1"] <span>@Model</span> @Localizer["jumbotron2", LanguageService.LanguageDisplay]</h1>
            @await Html.PartialAsync("Partials/_SearchComponent")
        </div>
    </div>
</section>
